<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
	  <RootNamespace>SFA.DAS.Approvals.ServiceBusIntegrationTests</RootNamespace>
