<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;netstandard2.1</TargetFrameworks>
      <PackageId>SFA.DAS.UI.FrameworkHelpers</PackageId>
      <Title>Specflow DotNet Core UI framework helpers</Title>
      <Configurations>Debug;Release;Development</Configurations>
      <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
      <PackageVersion>1.0.2</PackageVersion>
      <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
      <Authors>arvinthseran; Imran Ali</Authors>
      <Company>SFA.DAS</Company>
      <Description>A generic helpers wrapper for Specflow UI Framework integrated with donet core</Description>
      <PackageProjectUrl>https://accounts.manage-apprenticeships.service.gov.uk/service/index</PackageProjectUrl>
      <RepositoryUrl>https://github.com/SkillsFundingAgency/DFE-Standardised-Test-Automation-Framework/tree/master/src/SFA.DAS.UI.FrameworkHelpers</RepositoryUrl>
	  <PackageLicenseExpression>MS-PL</PackageLicenseExpression>
      <NeutralLanguage>en</NeutralLanguage>
      <PackageTags>cosmosdb webdriver</PackageTags>
      <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
	<PackageReference Include="RestSharp" Version="106.6.10" />
	<PackageReference Include="SFA.DAS.NServiceBus" Version="16.0.7" />
	<PackageReference Include="SFA.DAS.Payments.ProviderPayments.Messages" Version="0.0.69" />
	<PackageReference Include="SpecFlow" Version="3.3.30" />
	<PackageReference Include="System.Runtime.InteropServices.RuntimeInformation" Version="4.3.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="3.1.5" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="3.1.5" />
    <PackageReference Include="Microsoft.Extensions.Configuration.FileExtensions" Version="3.1.5" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="3.1.5" />
    <PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="3.1.5" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="3.1.5" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="3.1.5" />
    <PackageReference Include="Dapper" Version="2.0.35" />
    <PackageReference Include="DotNetSeleniumExtras.WaitHelpers" Version="3.11.0" />
    <PackageReference Include="MongoDB.Driver" Version="2.11.2" />
    <PackageReference Include="NUnit" Version="3.12.0" />
    <PackageReference Include="Polly" Version="7.1.0" />
    <PackageReference Include="Selenium.Support" Version="3.141.0" />
    <PackageReference Include="Selenium.WebDriver" Version="3.141.0" />
    <PackageReference Include="SFA.DAS.CosmosDb" Version="1.0.47" />
    <PackageReference Include="System.Data.SqlClient" Version="4.6.1" />
  </ItemGroup>

</Project>