	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="MailinatorApiClient" Version="1.0.2" />
		<PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="117.0.5938.8800" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\SFA.DAS.API.FrameworkHelpers\SFA.DAS.API.FrameworkHelpers.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="appsettings.Mailinator.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	</ItemGroup>


</Project>
